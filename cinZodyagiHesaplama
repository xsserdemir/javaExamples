import java.util.Scanner;
public class cinZodyagiHesaplama {
    public static void main(String[] args){
        Scanner inp = new Scanner(System.in);

        int year, value;

        System.out.print("Doğum Yılınızı Girin : ");
        year = inp.nextInt();

        value = year % 12;

        String zodiacSign = "";
        switch (value) {
            case 0:
                zodiacSign = ("Maymun");
                break;
            case 1:
                zodiacSign = ("Horoz");
                break;
            case 2:
                zodiacSign = ("Köpek");
                break;
            case 3:
                zodiacSign = ("Domuz");
                break;
            case 4:
                zodiacSign = ("Fare");
                break;
            case 5:
                zodiacSign = ("Öküz");
                break;
            case 6:
                zodiacSign = ("Kaplan");
                break;
            case 7:
                zodiacSign = ("Tavşan");
                break;
            case 8:
                zodiacSign = ("Ejderha");
                break;
            case 9:
                zodiacSign = ("Yılan");
            case 10:
                zodiacSign = ("At");
                break;
            case 11:
                zodiacSign = ("Koyun");
                break;
            default:
                System.out.println("Geçersiz Değer girdiniz");
        } System.out.println("Çin Zodyağı Burcunuz : " + zodiacSign);

    }
}
